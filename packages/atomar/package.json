{
  "name": "atomar",
  "version": "1.0.0-alpha.1",
  "description": "Atomar is a simple library based on rxjs and provides atoms and lenses concept (like haskel's lenses) and glue for react. Inspired by Focal and Rixio",
  "main": "esm/index.js",
  "types": "esm/index.d.ts",
  "module": "esm",
  "author": "Abdulla Bayramov",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@atomrx/atom": "^1.0.0-alpha.0",
    "@atomrx/form": "^1.0.0-alpha.0",
    "@atomrx/lens": "^1.0.0-alpha.0",
    "@atomrx/react": "^1.0.0-alpha.1",
    "@atomrx/utils": "^1.0.0-alpha.0",
    "@atomrx/wrapped": "^0.1.1"
  },
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rm -rf esm && rm -rf src/*.{js,d.ts,map}",
    "compile": "tsc -p tsconfig.build.json",
    "test": "jest"
  }
}
